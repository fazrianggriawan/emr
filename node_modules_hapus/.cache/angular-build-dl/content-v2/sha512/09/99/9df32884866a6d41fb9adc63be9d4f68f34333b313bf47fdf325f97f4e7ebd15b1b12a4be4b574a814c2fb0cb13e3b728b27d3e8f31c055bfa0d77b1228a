ikan.splice(i,1)\n  }\n\n  search(event:any){\n    //in a real application, make a request to a remote url with the query and return filtered results, for demo we filter at client side\n    let filtered : any[] = [];\n    let query = event.query;\n\n    for(let i = 0; i < this.masterObat.length; i++) {\n        let obat = this.masterObat[i];\n        if (obat.nama_obat.toLowerCase().indexOf(query.toLowerCase()) == 0) {\n            filtered.push(obat);\n        }\n    }\n    this.filterDataObat = filtered;\n  }\n\n  ngOnInit(): void {\n    this.obatService.getAllObat().subscribe(data => {\n      this.masterObat = data;\n    })\n\n    this.getDataHari();\n    this.getDataSigna2();\n\n  }\n\n}\n","<div class=\"p-mb-2\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-group row bd-b p-mb-2 p-pb-2\">\n            <label for=\"inputEmail3\" class=\"col-3 col-form-label\">Tipe Pemeriksaan</label>\n            <div class=\"col\">\n                <p-selectButton [options]=\"cito\" [(ngModel)]=\"selectedCito\" optionLabel=\"name\" optionValue=\"id\"></p-selectButton>\n            </div>\n        </div>\n        <div class=\"form-group row bd-b p-mb-2 p-pb-2\">\n            <label for=\"inputEmail3\" class=\"col-3 col-form-label\">Diagnosa Klinik</label>\n            <div class=\"col\">\n                <input type=\"text\" pInputText class=\"form-control\">\n            </div>\n        </div>\n        <div class=\"form-group row bd-b p-mb-2 p-pb-2\">\n            <label for=\"inputEmail3\" class=\"col-3 col-form-label\">ICD 10</label>\n            <div class=\"col\">\n                <p-dropdown [options]=\"icd10\" optionLabel=\"name\" placeholder=\"Pilih...\" [virtualScroll]=\"true\" [itemSize]=\"31\" [filter]=\"true\" [showClear]=\"true\"></p-dropdown>\n            </div>\n        </div>\n    </fieldset>\n</div>\n<div class=\"card-columns\" *ngIf=\"selectedCito=='cito'\">\n    <div class=\"card rounded bd\" *ngFor=\"let item of dataMasterLabCito; let i 